<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="propertyDetailPanel" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PropertyDetailWindow">
   <children>
      <Text fx:id="mytext" layoutX="286.0" layoutY="205.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <Button fx:id="bclose" layoutX="37.0" layoutY="37.0" mnemonicParsing="false" onAction="#ok" text="Back" />
      <Button fx:id="brent" layoutX="37.0" layoutY="93.0" mnemonicParsing="false" onAction="#rent" text="Rent" />
      <Button layoutX="122.0" layoutY="93.0" mnemonicParsing="false" text="Return" />
      <Button layoutX="221.0" layoutY="93.0" mnemonicParsing="false" text="Maintaince" />
      <Button layoutX="346.0" layoutY="93.0" mnemonicParsing="false" text="Complete Maintaince" />
      
      <TableView fx:id="rentalTable">
		        <columnResizePolicy>
		        	<TableView fx:constant="CONSTRAINED_RESIZE_POLICY"></TableView>
		        </columnResizePolicy>
		            <columns>
		                <TableColumn text="Property Id">
		                    <cellValueFactory>
		                        <PropertyValueFactory property="property_Id"/>
		                    </cellValueFactory>
		                </TableColumn>
		                 <TableColumn text="Street Number">
		                    <cellValueFactory>
		                        <PropertyValueFactory property="street_num"/>
		                    </cellValueFactory>
		                </TableColumn>
		                 <TableColumn text="Street Name">
		                    <cellValueFactory>
		                        <PropertyValueFactory property="street_name"/>
		                    </cellValueFactory>
		                </TableColumn>
		                 <TableColumn text="Suburb">
		                    <cellValueFactory>
		                        <PropertyValueFactory property="suburb"/>
		                    </cellValueFactory>
		                </TableColumn>
		                <TableColumn text="NumOfBeds">
		                    <cellValueFactory>
		                        <PropertyValueFactory property="num_of_beds"/>
		                    </cellValueFactory>
		                </TableColumn>
		                 <TableColumn text="Property Status">
		                    <cellValueFactory>
		                        <PropertyValueFactory property="property_status"/>
		                    </cellValueFactory>
		                </TableColumn>
		                <TableColumn text="Property Type">
		                    <cellValueFactory>
		                        <PropertyValueFactory property="property_type"/>
		                    </cellValueFactory>
		                </TableColumn>
		                <TableColumn text="LastMaintainance Date">
		                    <cellValueFactory>
		                        <PropertyValueFactory property="lastMaintenanceDate"/>
		                    </cellValueFactory>
		                </TableColumn>
		            </columns>
        </TableView>
      
      
   </children>
</AnchorPane>
